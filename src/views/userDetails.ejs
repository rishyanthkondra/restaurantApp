                                    <%}else{%>
                                    <option value="home">home</option>
                                    <%}%><% if(addresses[i].symbol == "work"){%>
                                    <option value="work" selected>work</option>
                                    <%}else{%>
                                    <option value="work">work</option>
                                    <%}%><% if(addresses[i].symbol == "other"){%>
                                    <option value="other" selected>other</option>
                                    <%}else{%>
                                    <option value="other">other</option>
                                    <%}%>
                                </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="house_num">House Number</label>
                                <div class="col-sm-10">
                                <input class= "form-control" type="text" name="house_num" minlength="5" id="alias" value="<%= addresses[i].house_num %>">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="region">Region/Locality</label>
                                <div class="col-sm-10">
                                <input class= "form-control" type="text" name="region" minlength="5" id="region" value="<%= addresses[i].region %>">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="area_code">Primary Code</label>
                                <div class="col-sm-10">
                                <input type="number" class="form-control" min = 0 max = 5 
                                id="area_code" name="area_code"  <% if(addresses[i].primarycode!=null){ %> value="<%=addresses[i].primarycode%>" <%}%> required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <button class="btn btn-primary btn-block" type="submit"
                                onclick="return confirm('Are you sure you want to update <%= addresses[i].alias%>?')">
                                Update</button>
                                <button class="btn btn-primary btn-block" type="submit"
                                onclick="return confirm('Are you sure you want to delete <%= addresses[i].alias%>?')"
                                formaction="userDetails/delete-address/?address_id=<%=addresses[i].address_id%>">
                                Delete</button>
                            </div>
                        </form>
                        <br>
                    </div>
                </div>
            </div>
            <% } %>
        </div>
        <br>
        <div class="row" id="addAddress">
            <div class="col" align="center">
                <div align="center" style="background-color:rgba(0, 0, 0, 0.5);border-radius: 1vw;">
                    <div align="center" style="width: 50%;">
                        <p class="mbr-text mbr-fonts-style display-7" align="top"><strong><br>Add Address</strong></p>
                        <form action="userDetails/add-address" method="POST">
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="alias">Place Name</label>
                                <div class="col-sm-10">
                                <input class= "form-control" type="text" name="alias" minlength="5" id="alias" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="symbol">Select Symbol</label>
                                <div class="col-sm-10">
                                <select class= "form-control" id="symbol" name="symbol">
                                    <option value="home" selected>home</option>
                                    <option value="work">work</option>
                                    <option value="other">other</option>
                                </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="house_num">House Number</label>
                                <div class="col-sm-10">
                                <input class= "form-control" type="text" name="house_num" minlength="5" id="alias" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="region">Region/Locality</label>
                                <div class="col-sm-10">
                                <input class= "form-control" type="text" name="region" minlength="5" id="region" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="area_code">Primary Code</label>
                                <div class="col-sm-10">
                                <input type="number" class="form-control" min = 0 max = 5 
                                id="area_code" name="area_code" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <button class="btn btn-primary btn-block" type="submit"
                                onclick="return confirm('Adding new address')">
                                Add</button>
                            </div>
                        </form>
                        <br>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


