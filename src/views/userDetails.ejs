<div class="header11 cid-swZHoHlbUv mbr-fullscreen mbr-parallax-background" id="header11-o">
    

    <div class="mbr-overlay" style="opacity: 0.3; background-color: rgb(0, 0, 0);"></div>
    
    <div class="container">
        <div class="row">
            <div class="col" align="center" id="personaldetails">
                <p class="mbr-text mbr-fonts-style display-7" align="top"><strong>Personal Details</strong></p>
                <div align="center" style="background-color:rgba(0, 0, 0, 0.5);border-radius: 1vw;width: 80%;">
                    <br>
                    <div style="width:15vw;height:15vw;border-radius:7.5vw;
                        overflow:hidden;">
                        <img src="<%=userImage%>" style="width:12vw;height:14.5vw">
                    </div>
                    <br>
                    <div align="center" style="width: 50%;">
                        <form action="userDetails/edit-details%>" 
                            method="POST">
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="first_name">First Name</label>
                                <div class="col-sm-10">
                                <input type="text" class="form-control" name="first_name" id="first_name" value="<%= first_name %>" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="last_name">Last Name</label>
                                <div class="col-sm-10">
                                <input type="text" class="form-control" name="last_name" id="last_name" value="<%= last_name %>" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-4 display-7" for="email">Email Address</label>
                                <br>
                                <div class="col-sm-10">
                                <input type="text" class="form-control" name="email" id="email" value="<%= email %>" readonly>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="phone_number">Phone Number</label>
                                <div class="col-sm-10">
                                <input type="number" class="form-control"  maxlength="15" minlength="10" 
                                id="phone_number" name="phone_number" value= <%= phone_number %> required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="date_of_birth">Date of Birth</label>
                                <div class="col-sm-10">
                                <input type="date" class="form-control" max="2016-01-01" id="date_of_birth" name="date_of_birth" value= <%= date_of_birth %> required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="gender">Select Gender</label>
                                <div class="col-sm-10">
                                <select class="form-control" id="gender" name="gender">
                                     <% if(gender == "male"){%>
                                    <option value="male" selected>male</option>
                                    <%}else{%>
                                    <option value="male">male</option>
                                    <%}%><% if(gender == "female"){%>
                                    <option value="female" selected>female</option>
                                    <%}else{%>
                                    <option value="female">female</option>
                                    <%}%><% if(gender == "other"){%>
                                    <option value="other" selected>other</option>
                                    <%}else{%>
                                    <option value="other">other</option>
                                    <%}%>
                                </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-10">
                                <button class="btn btn-primary btn-block" type="submit"
                                onclick="return confirm('Updating Details')">Update Details</button>
                                </div>
                            </div>
                            </form>
                            <br>
                    </div>
                </div>
            </div>
        </div>    
        <br><br>
        <div class="row" id="addressDetails">
            <% for(var i=0; i < addresses.length; i++) { %>
            <% if(i%2 == 0 && i>0){%>
                </div><br><br>
                <div class="row">
            <%}%>
            <div class="col" align="center">
                <div align="center" style="background-color:rgba(0, 0, 0, 0.5);border-radius: 1vw;">
                    <div align="center" style="width: 80%;">
                        <p class="mbr-text mbr-fonts-style display-7" align="top">
                            <span>
                                <%if(addresses[i].symbol=='home'){%>
                                    <i class="fas fa-home"></i>
                                <%}else if(addresses[i].symbol=='work'){%>
                                    <i class="fas fa-briefcase"></i>
                                <%}else{%>
                                    <i class="fas fa-user"></i>
                                <%}%>
                            </span>
                            <strong><br>Address Details</strong>
                        </p>
                        <form action="userDetails/edit-address/?address_id=<%=addresses[i].address_id%>" method="POST">
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="alias">Place Name</label>
                                <div class="col-sm-10">
                                <input class= "form-control" type="text" name="alias" minlength="5" id="alias" value="<%= addresses[i].alias %>">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="mbr-text mbr-fonts-style mb-2 display-7" for="symbol">Select Symbol</label>
                                <div class="col-sm-10">
                                <select class= "form-control" id="symbol" name="symbol">
